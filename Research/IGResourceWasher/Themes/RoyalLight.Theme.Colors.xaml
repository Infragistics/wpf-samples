  <Color x:Key="Color_030">#FF6E42B2</Color>
  <Color x:Key="Color_031">#FF855DC3</Color>
  <Color x:Key="Color_032">#FF9F7FCF</Color>
  <Color x:Key="Color_033">#FFB59DDB</Color>
  <Color x:Key="Color_034">#FFCFBFE7</Color>

  <!--Whites with transparency-->
  <Color x:Key="Color_040">#E5FFFFFF</Color><!--90% White-->
  <Color x:Key="Color_041">#BFFFFFFF</Color><!--75% White-->
  <Color x:Key="Color_042">#99FFFFFF</Color><!--60% White-->
