  <!--Colors for Chart Series --> 
  <Color x:Key="Color_020">#FF7446B9</Color>
  <Color x:Key="Color_021">#FF9FB328</Color>
  <Color x:Key="Color_022">#FFF96232</Color>
  <Color x:Key="Color_023">#FF2E9CA6</Color>
  <Color x:Key="Color_024">#FFDC3F76</Color>
  <Color x:Key="Color_025">#FFFF9800</Color>
  <Color x:Key="Color_026">#FF3F51B5</Color>
  <Color x:Key="Color_027">#FF439C47</Color>
  <Color x:Key="Color_028">#FF795548</Color>
  <Color x:Key="Color_029">#FF9A9A9A</Color>

