    <Grid x:Name="LayoutRoot">
        <Grid x:Name="windowContainer">
            <Grid x:Name="ContainerGrid" Grid.Column="0">
                <Grid.Resources>
                    <DataTemplate x:Key="HeaderIcon">
                        <Image Source="/IGDialogWindow;component/Images/CustomIcon.png" Height="45" Width="45" />
                    </DataTemplate>
                </Grid.Resources>
                <!-- #BEGIN SNIPPET# -->
                <ig:XamDialogWindow x:Name="xamWindow"
                    Width="350"
                    Height="200"
                    HeaderIconTemplate="{StaticResource HeaderIcon}" Style="{StaticResource XamDialogWindowStyle}" StartupPosition="Center"
                    Header="{Binding Source={StaticResource DWStrings}, Path=XDW_ContainerWindow}"
                />
                <!-- #END SNIPPET# -->
            </Grid>
            <Button
                HorizontalAlignment="Center"
                VerticalAlignment="Center"
                Padding="5"
                Content="{Binding Source={StaticResource DWStrings}, Path=XDW_LaunchWindow}"
                Click="Button_Click"
                Visibility="{Binding WindowState, ElementName=xamWindow, Converter={StaticResource windowStateToVisibilityConverter}}"
            />
        </Grid>
    </Grid>
