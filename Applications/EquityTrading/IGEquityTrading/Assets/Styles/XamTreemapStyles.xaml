                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="OverlayBorder" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <EasingDoubleKeyFrame KeyTime="0" Value="0.3"/>
