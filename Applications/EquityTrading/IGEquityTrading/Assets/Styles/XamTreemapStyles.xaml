                                <win:VisualState x:Name="ReadOnly"/>
                            </win:VisualStateGroup>
                            <win:VisualStateGroup x:Name="FocusStates">
                                <win:VisualState x:Name="Focused"/>
                                <win:VisualState x:Name="Unfocused"/>
                            </win:VisualStateGroup>
                        </win:VisualStateManager.VisualStateGroups>
                        <Border.ToolTip>
