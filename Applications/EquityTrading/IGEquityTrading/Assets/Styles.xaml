    <!-- ListBox Item Style-->
    <Style x:Key="StocksListBoxItemStyle1" TargetType="{x:Type ListBoxItem}">
        <Setter Property="Padding" Value="3" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="Background" Value="Transparent" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type ListBoxItem}">
                    <Grid Background="{TemplateBinding Background}">
                        <Grid Height="37" MinWidth="84" Margin="0" VerticalAlignment="Center">
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="18" />
                                <ColumnDefinition MinWidth="90" />
                                <ColumnDefinition Width="66" />
                            </Grid.ColumnDefinitions>
                            <Border x:Name="leftSide" Grid.ColumnSpan="2" Background="{DynamicResource TabNormalFill}" BorderBrush="{x:Null}" CornerRadius="8,0,0,0" />
                            <StackPanel Grid.Column="1" Margin="0,10" Orientation="Horizontal" d:LayoutOverrides="Width">
                                <TextBlock Width="Auto" Margin="0" VerticalAlignment="Center" Cursor="Hand" FontSize="12" FontWeight="Bold" Foreground="White" Text="{Binding Symbol}" TextWrapping="Wrap" />
                                <TextBlock Width="Auto" HorizontalAlignment="Left" Margin="7,0,0,0" VerticalAlignment="Center" Cursor="Hand" FontFamily="Arial" FontSize="12" Foreground="White" Text="{Binding Details.LastTradeAmount, Mode=OneWay}" TextWrapping="Wrap" />
                            </StackPanel>
                            <Canvas x:Name="canvas" Height="37" Grid.Column="2" VerticalAlignment="Top">
                                <Path x:Name="rightSide" Width="66" Height="37" HorizontalAlignment="Right" Data="M111.87,5.68E-14 C111.87,5.68E-14 156.03,5.68E-14 156.03,5.68E-14 C164.40,0.12 166.65,5.08 169.90,12.28 L178,37 C178,37 143.41,37 111.87,37 C111.87,37 111.87,5.68E-14 111.87,5.68E-14 z" Fill="{DynamicResource TabNormalFill}" Stretch="Fill" UseLayoutRounding="False" />
                                <ToggleButton x:Name="toggleButton" Width="14" Height="10" Canvas.Left="11" Canvas.Top="13" Content="ToggleButton" Cursor="Hand" Style="{StaticResource StockNameToggleButton}" Visibility="Collapsed" />
                                <Button x:Name="button" Width="9" Height="9" Canvas.Left="36" Canvas.Top="14" Margin="-6,0,0,0" Background="{x:Null}" BorderBrush="{x:Null}" BorderThickness="0" Command="{Binding Parent.RemoveStockCommand}" CommandParameter="{Binding Symbol}" Style="{StaticResource StockTemplateButton}" />
                            </Canvas>
                        </Grid>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="true">
                            <Setter TargetName="leftSide" Property="Background" Value="{DynamicResource TabHoverFill}" />
                            <Setter TargetName="rightSide" Property="Fill" Value="{DynamicResource TabHoverFill}" />
                        </Trigger>
                        <Trigger Property="IsSelected" Value="true">
                            <Setter TargetName="leftSide" Property="Background" Value="{DynamicResource ItemSelectedFill}" />
                            <Setter TargetName="rightSide" Property="Fill" Value="{DynamicResource ItemSelectedFill}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="true" />
                                <Condition Property="Selector.IsSelectionActive" Value="false" />
                            </MultiTrigger.Conditions>
                        </MultiTrigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

