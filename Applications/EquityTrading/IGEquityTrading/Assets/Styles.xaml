    <!-- RadioButton Style -->
    <Style x:Key="StockTimeSelctorRadioButtonStyle" TargetType="{x:Type RadioButton}">
        <Setter Property="Background" Value="#FF448DCA" />
        <Setter Property="Foreground" Value="White" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="Width" Value="45" />
        <Setter Property="Height" Value="50" />
        <Setter Property="MinWidth" Value="0" />
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="FontSize" Value="16" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <GradientStop Offset="0" Color="#FFA3AEB9" />
                    <GradientStop Offset="0.37" Color="#FF8399A9" />
                    <GradientStop Offset="0.37" Color="#FF718597" />
                    <GradientStop Offset="1" Color="#FF617584" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <Grid Height="32" Cursor="Hand">
                        <presentationFramework:VisualStateManager.VisualStateGroups>
                            <presentationFramework:VisualStateGroup x:Name="CommonStates">
                                <presentationFramework:VisualState x:Name="Normal" />
                                <presentationFramework:VisualState x:Name="MouseOver" />
                                <presentationFramework:VisualState x:Name="Pressed" />
                                <presentationFramework:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="contentPresenter" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value=".55" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="CheckStates">
                                <presentationFramework:VisualState x:Name="Checked">
                                    <Storyboard>
                                        <ObjectAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="border" Storyboard.TargetProperty="(UIElement.Visibility)">
                                            <DiscreteObjectKeyFrame KeyTime="00:00:00">
                                                <DiscreteObjectKeyFrame.Value>
                                                    <Visibility>Visible
                                                    </Visibility>
                                                </DiscreteObjectKeyFrame.Value>
                                            </DiscreteObjectKeyFrame>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Unchecked" />
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="FocusStates">
                                <presentationFramework:VisualState x:Name="Focused" />
                                <presentationFramework:VisualState x:Name="Unfocused" />
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="ValidationStates">
                                <presentationFramework:VisualState x:Name="Valid" />
                                <presentationFramework:VisualState x:Name="InvalidUnfocused" />
                                <presentationFramework:VisualState x:Name="InvalidFocused" />
                            </presentationFramework:VisualStateGroup>
                        </presentationFramework:VisualStateManager.VisualStateGroups>
                        <Border x:Name="border" Margin="0,1,0,0" Background="Black" Visibility="Collapsed" />
                        <Grid HorizontalAlignment="Left" VerticalAlignment="Top" />
                        <ContentPresenter x:Name="contentPresenter" Grid.Column="1" HorizontalAlignment="Center" Margin="13,0" VerticalAlignment="Center" Content="{TemplateBinding Content}" ContentTemplate="{TemplateBinding ContentTemplate}" Visibility="Collapsed" />
                        <TextBlock HorizontalAlignment="Center" Margin="13,0" VerticalAlignment="Center" FontFamily="Arial" FontSize="12" Text="{TemplateBinding Content}" d:LayoutOverrides="Width" />
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Setter Property="Margin" Value="10,0" />
    </Style>

