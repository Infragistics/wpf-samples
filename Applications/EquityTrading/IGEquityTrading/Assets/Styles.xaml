    <!-- ChartType RadioButton Style -->
    <Style x:Key="ChartType_RadioButton" TargetType="{x:Type RadioButton}">
        <Setter Property="Background" Value="#FF448DCA" />
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="HorizontalContentAlignment" Value="Left" />
        <Setter Property="VerticalContentAlignment" Value="Top" />
        <Setter Property="Padding" Value="4,1,0,0" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="BorderBrush">
            <Setter.Value>
                <LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
                    <GradientStop Offset="0" Color="#FFA3AEB9" />
                    <GradientStop Offset="0.37" Color="#FF8399A9" />
                    <GradientStop Offset="0.37" Color="#FF718597" />
                    <GradientStop Offset="1" Color="#FF617584" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type RadioButton}">
                    <Grid Width="46" Height="26">
                        <presentationFramework:VisualStateManager.VisualStateGroups>
                            <presentationFramework:VisualStateGroup x:Name="CommonStates">
                                <presentationFramework:VisualStateGroup.Transitions>
                                    <presentationFramework:VisualTransition GeneratedDuration="0:0:0.3" />
                                </presentationFramework:VisualStateGroup.Transitions>
                                <presentationFramework:VisualState x:Name="Normal" />
                                <presentationFramework:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation d:IsOptimized="True" Duration="0" Storyboard.TargetName="path" Storyboard.TargetProperty="(UIElement.Opacity)" To="0.6" />
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="pressed" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.5" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <PointAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="pressed" Storyboard.TargetProperty="(Shape.Fill).(RadialGradientBrush.Center)">
                                            <EasingPointKeyFrame KeyTime="00:00:00" Value="0.45,0.99" />
                                        </PointAnimationUsingKeyFrames>
                                        <PointAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="pressed" Storyboard.TargetProperty="(Shape.Fill).(RadialGradientBrush.GradientOrigin)">
                                            <EasingPointKeyFrame KeyTime="00:00:00" Value="0.45,0.99" />
                                        </PointAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="pressed" Storyboard.TargetProperty="(Shape.Fill).(RadialGradientBrush.RadiusX)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.83" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames Storyboard.TargetName="contentPresenter" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <SplineDoubleKeyFrame KeyTime="0" Value=".55" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="CheckStates">
                                <presentationFramework:VisualState x:Name="Checked">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="active" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.5" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="canvas" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0.3" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Unchecked" />
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="FocusStates">
                                <presentationFramework:VisualState x:Name="Focused">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.00" Storyboard.TargetName="focused" Storyboard.TargetProperty="(UIElement.Opacity)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Unfocused" />
