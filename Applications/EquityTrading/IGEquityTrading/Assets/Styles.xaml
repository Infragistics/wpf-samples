    <!-- Stock Template Text-->
    <Style x:Key="StockTemplateText" TargetType="{x:Type TextBlock}">
        <Setter Property="FontSize" Value="12" />
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Arial" />
        <Setter Property="Margin" Value="0" />
        <Setter Property="TextWrapping" Value="Wrap" />
        <Setter Property="Cursor" Value="Hand" />
    </Style>
    <!-- DataTemplate for Stock List Box instances -->
    <DataTemplate x:Key="StockTemplate">
        <StackPanel Height="37" MinWidth="84" Width="Auto" HorizontalAlignment="Left" Margin="0" VerticalAlignment="Top" Orientation="Horizontal">
            <TextBlock Height="37" MinWidth="84" Width="Auto" Foreground="{Binding Brush}" Style="{StaticResource StockTemplateText}" Text="{Binding Symbol}" />
        </StackPanel>
    </DataTemplate>
    <ItemsPanelTemplate x:Key="ListBoxItemsPanelTemplateH">
        <StackPanel HorizontalAlignment="Center" Orientation="Horizontal" />
    </ItemsPanelTemplate>
   
