    <!-- Add Button Style-->
    <Style x:Key="AddButton" TargetType="{x:Type Button}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type Button}">
                    <Grid Width="58" Height="40" Cursor="Hand">
                        <presentationFramework:VisualStateManager.VisualStateGroups>
                            <presentationFramework:VisualStateGroup x:Name="FocusStates">
                                <presentationFramework:VisualState x:Name="Focused" />
                                <presentationFramework:VisualState x:Name="Unfocused" />
                            </presentationFramework:VisualStateGroup>
                            <presentationFramework:VisualStateGroup x:Name="CommonStates">
                                <presentationFramework:VisualState x:Name="Normal" />
                                <presentationFramework:VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <DoubleAnimation d:IsOptimized="True" Duration="0:0:0.3" Storyboard.TargetName="over" Storyboard.TargetProperty="(UIElement.Opacity)" To="1" />
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="0:0:0.1" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Offset)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="1" />
                                        </DoubleAnimationUsingKeyFrames>
                                        <DoubleAnimationUsingKeyFrames BeginTime="00:00:00" Duration="0:0:0.1" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[0].(GradientStop.Offset)">
                                            <EasingDoubleKeyFrame KeyTime="00:00:00" Value="0" />
                                        </DoubleAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                                <presentationFramework:VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="0" Storyboard.TargetName="rectangle" Storyboard.TargetProperty="(Shape.Fill).(GradientBrush.GradientStops)[1].(GradientStop.Color)">
                                            <EasingColorKeyFrame KeyTime="00:00:00" Value="#FF1A1919" />
                                        </ColorAnimationUsingKeyFrames>
                                    </Storyboard>
                                </presentationFramework:VisualState>
                            </presentationFramework:VisualStateGroup>
                        </presentationFramework:VisualStateManager.VisualStateGroups>
                        <Path x:Name="rectangle" Data="M0.49,0.5 C0.49,0.5 36.16,0.5 50.5,0.5 C54.36,0.5 57.5,3.63 57.5,7.5 L57.5,29.5 C57.5,33.36 54.36,36.5 50.5,36.5 C36.16,36.5 0.49,36.5 0.49,36.5 C0.49,36.5 0.49,0.5 0.49,0.5 z" Stretch="Fill" Stroke="Black" UseLayoutRounding="False">
                            <Path.Fill>
                                <LinearGradientBrush StartPoint="0.5,0.07" EndPoint="0.5,0.94">
                                    <GradientStop Offset="1" Color="Black" />
                                    <GradientStop Color="#FF333333" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <Path x:Name="over" Data="M0.49,0.5 C0.49,0.5 36.16,0.5 50.5,0.5 C54.36,0.5 57.5,3.63 57.5,7.5 L57.5,29.5 C57.5,33.36 54.36,36.5 50.5,36.5 C36.16,36.5 0.49,36.5 0.49,36.5 C0.49,36.5 0.49,0.5 0.49,0.5 z" Opacity="0" Stretch="Fill" Stroke="Black" UseLayoutRounding="False">
                            <Path.Fill>
                                <LinearGradientBrush StartPoint="0.5,0.07" EndPoint="0.5,0.94">
                                    <GradientStop Offset="1" Color="Black" />
                                    <GradientStop Color="#FF5D5D5D" />
                                </LinearGradientBrush>
                            </Path.Fill>
                        </Path>
                        <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}">
                            <ContentPresenter.Content>
                                <TextBlock FontFamily="Arial" FontSize="14" FontWeight="Bold" Foreground="White" Text="{TemplateBinding Content}" TextWrapping="NoWrap" />
                            </ContentPresenter.Content>
                        </ContentPresenter>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

