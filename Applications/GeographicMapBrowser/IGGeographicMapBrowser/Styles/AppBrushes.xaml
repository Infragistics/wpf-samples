    xmlns:common="clr-namespace:IGExtensions.Common.Controls;assembly=IGExtensions.Common" mc:Ignorable="d">

    <!--this ResourceDictionary should contain only brush resources -->
    <!--In order to exclude a brush from washing using the ResourceWahser, add these to properties to individual brushes:
    common:ColorWasher.WashGroup="LightWashGroup" 
    common:ColorWasher.IsExcludedFromWash="True"  
    -->  
    <Color x:Key="AirportEllipseCenterFillColor">#FF134886</Color>
    <Color x:Key="AirportEllipseFillColor">#FF1161BF</Color>

    <Color x:Key="AirplaneFillColor">#FF1161BF</Color>
    <Color x:Key="AirplaneStrokeColor">#FF114684</Color>
    <SolidColorBrush x:Key="AirportEllipseCenterFillBrush" Color="{StaticResource AirportEllipseCenterFillColor}" />
    <SolidColorBrush x:Key="AirportEllipseFillBrush" Color="{StaticResource AirportEllipseFillColor}" />
    <SolidColorBrush x:Key="AirportEllipseStrokeBrush" Color="{StaticResource AirportEllipseCenterFillColor}" />

    <SolidColorBrush x:Key="AirplaneFillBrush" Color="{StaticResource AirplaneFillColor}" />
    <SolidColorBrush x:Key="AirplaneStrokeBrush" Color="{StaticResource AirplaneStrokeColor}" />
    <SolidColorBrush x:Key="AirplaneTooltipBackground" Color="{StaticResource Color_001}"/>
    <SolidColorBrush x:Key="AirplaneTooltipBorderBrush" Color="#FFD5EAF2"/>

