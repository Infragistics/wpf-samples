    <LinearGradientBrush x:Key="HorizontalRepeatBtnFill" EndPoint="0.81,0.80" StartPoint="0.81,0.03">
        <LinearGradientBrush.GradientStops>
            <GradientStopCollection>
                <GradientStop Color="#FFfcfbfb" Offset="0"/>
