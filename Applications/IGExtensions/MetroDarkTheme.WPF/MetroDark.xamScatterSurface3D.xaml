<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:System="clr-namespace:System;assembly=mscorlib"
    xmlns:shared="http://schemas.infragistics.com/xaml" 
    xmlns:charts="http://schemas.infragistics.com/xaml">

    <!-- *********************************  RESOURCES  ********************************* -->
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="Theme.Colors.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <!-- ********************************** BRUSHES **********************************-->
    <SolidColorBrush x:Key="CubeMaterialBrush" Color="{StaticResource Color_0151}"/>
    <SolidColorBrush x:Key="GridStrokeColor" Color="{StaticResource Color_006}"/>
    <SolidColorBrush x:Key="AxisStrokeColor" Color="{StaticResource Color_006}"/>

    <SolidColorBrush x:Key="LabelForeground" Color="{StaticResource Color_001}"/>
  	<SolidColorBrush x:Key="TitleForeground" Color="{StaticResource Color_001}"/>	
    <SolidColorBrush x:Key="ChartBackground" Color="{StaticResource Color_009}"/>
    <SolidColorBrush x:Key="PointMarkerMaterialBrush" Color="{StaticResource Color_035}"/>
    <SolidColorBrush x:Key="CrosshairBrush" Color="{StaticResource Color_035}"/>
    <Color x:Key="AmbientLightColor">Black</Color>

    <!-- Tooltip Brushes -->
    <SolidColorBrush x:Key="TooltipBackgroundBrush" Color="{StaticResource Color_009}" />
    <SolidColorBrush x:Key="TooltipBorderBrush" Color="{StaticResource Color_006}" />
    <SolidColorBrush x:Key="TooltipForeground" Color="{StaticResource Color_001}"/>

    <!-- *********************************  Chart Series Colors ********************************* -->
    <charts:ColorCollection x:Key="SeriesColorsCollection" Capacity="4">
        <StaticResource ResourceKey="Color_023"/>
        <StaticResource ResourceKey="Color_024"/>
        <StaticResource ResourceKey="Color_037"/>
    </charts:ColorCollection>

    <!-- *********************************  Font Styles  ********************************* -->
    <System:Double x:Key="LabelFontSize">11</System:Double>
    <System:Double x:Key="TitleFontSize">12</System:Double>
    <System:Double x:Key="LabelOffset">0.02</System:Double>
    <System:Double x:Key="TitleOffset">0.02</System:Double>

    <!-- *********************************  Strokes and Spacing  ********************************* -->
    <System:Double x:Key="CrosshairThickness">2</System:Double>
    <System:Double x:Key="GridStrokeThickness">1</System:Double>
    <System:Double x:Key="AxisStrokeThickness">2</System:Double>
    <Thickness x:Key="TooltipBorderThickness">1</Thickness>
    <Thickness x:Key="TooltipContentPadding">8,6</Thickness>

