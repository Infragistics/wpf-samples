    <Style x:Key="EarthquakeAxisStyle" TargetType="ig:DateTimeAxis">
        <Setter Property="IsTabStop" Value="False"/>
        <Setter Property="ShowLabels" Value="False" />
        <Setter Property="ShowMajorTickMarks" Value="False" />
        <Setter Property="ThumbStyle">
            <Setter.Value>
                <Style TargetType="ig:AxisThumb">
                    <Setter Property="Width" Value="40" />
                    <Setter Property="Height" Value="40" />
                    <Setter Property="Canvas.ZIndex" Value="0" />
