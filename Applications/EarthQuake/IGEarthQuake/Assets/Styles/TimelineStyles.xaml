                                                        <VisualState x:Name="MouseOver">
                                                        	<Storyboard>
                                                        		<DoubleAnimation Duration="0" To="0.5" Storyboard.TargetProperty="(UIElement.Opacity)" Storyboard.TargetName="border" d:IsOptimized="True"/>
