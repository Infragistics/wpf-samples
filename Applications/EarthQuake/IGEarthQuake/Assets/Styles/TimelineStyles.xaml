		 <GradientStop Offset="0.249" Color="#FFE6E6E6" />
        <GradientStop Offset="0.25" Color="#FFEEEEEE"/>
		<GradientStop Offset="0.399" Color="#FFEEEEEE"/>
        <GradientStop Offset="0.4" Color="#FFF8F8F8" />
        <GradientStop Offset="0.6" Color="#FFF8F8F8" />
		<GradientStop Offset="0.601" Color="#FFF8F8F8" />
        <GradientStop Offset="0.75" Color="#FFF8F8F8" />
		 <GradientStop Offset="0.751" Color="#FFEEEEEE" />
        <GradientStop Offset="0.9" Color="#FFEEEEEE" />
    </LinearGradientBrush>

