    <!-- Magnitude Gauge Style -->
    <Style x:Key="MagnitudeGaugeStyle" TargetType="ig:XamLinearGauge">
		<Setter Property="BorderBrush" Value="#FF617583"/>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="ig:XamLinearGauge">
					<Grid>
						<Rectangle Fill="#FF666666" Height="8" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
							<Rectangle.RenderTransform>
								<TranslateTransform Y="1"/>
							</Rectangle.RenderTransform>
						</Rectangle>
						<Rectangle Fill="#FF666666" Height="8" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5">
							<Rectangle.RenderTransform>
								<TranslateTransform Y="-1"/>
							</Rectangle.RenderTransform>
						</Rectangle>
						<Rectangle  Height="8" VerticalAlignment="Center" RenderTransformOrigin="0.5,0.5" Margin="1 0">
							<Rectangle.Fill>
								<LinearGradientBrush EndPoint="1.0,0.5" StartPoint="0.0,0.5">
                                    <GradientStop Color="#FFF5E3A8" Offset="0"/>
                                    <GradientStop Color="#FFFFD034" Offset="0.2"/>
                                    <GradientStop Color="#FFFC8612" Offset="0.4"/>
                                    <GradientStop Color="#FFED4840" Offset="0.6"/>
                                    <GradientStop Color="#FF7D120D" Offset="0.8"/>
                                    <GradientStop Color="#FF1F0504" Offset="1"/>
                                </LinearGradientBrush>
							</Rectangle.Fill>
						</Rectangle>
						<ContentPresenter x:Name="RootElement"/>
					</Grid>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>


