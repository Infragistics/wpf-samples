  <xs:complexType name="SamplesBrowser_TYPE">
    <!-- Elements -->
    <xs:sequence>
      <xs:element name="Flags" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Flag" minOccurs="1" maxOccurs="unbounded">
              <xs:complexType>
                <xs:attribute name="val" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
        <xs:unique name="UniqueFlags">
          <xs:selector xpath="tns:Flag" />
          <xs:field xpath="@val" />
        </xs:unique>
      </xs:element>
      <xs:element name="route" type="RouteType" minOccurs="0" maxOccurs="unbounded"/>
      <xs:element name="ControlGroups" type="ControlGroups_TYPE" minOccurs="1" maxOccurs="1" />
      <xs:element name="FeaturesPageEN" type="LocalizableText"  minOccurs="1" maxOccurs="unbounded"/>   
      <xs:element name="FeaturesPageJA" type="LocalizableText"  minOccurs="1" maxOccurs="unbounded"/>     
      <xs:element name="ShowcasesPageEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded"/>       
      <xs:element name="ShowcasesPageJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded"/>       
      <xs:element name="LinksPageEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded"/>        
      <xs:element name="LinksPageJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded"/>         
    </xs:sequence>
    <!-- Attributes -->
    <!--<xs:attribute name="ProductFamilyID" fixed="6" use="required" />-->
    <xs:attribute name="currentVersion" type="releaseVersionsEnum" use="required" />
  </xs:complexType>
  <xs:complexType name="ControlGroups_TYPE">
