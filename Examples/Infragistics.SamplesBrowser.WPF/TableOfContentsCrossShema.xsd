    </xs:sequence>
    <!--<xs:attribute name="name" type="categoriesEnum" use="required" />-->
    <xs:attribute name="name" type="xs:anyURI" use="required" />    
  </xs:complexType>
  <xs:complexType name="Sample_TYPE">
    <xs:sequence>
      <xs:element name="displayNameEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="displayNameJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="descriptionEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="descriptionJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="shortdescription" type="LocalizableText" minOccurs="0" maxOccurs="1" />
      <xs:element name="fallback" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="start" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="assembly" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="CodeFiles" minOccurs="0" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="CodeFile" minOccurs="1" maxOccurs="unbounded">
              <xs:complexType>
                <xs:attribute name="path" type="xs:anyURI" use="required" />
                <xs:attribute name="isStartUp" type="xs:boolean" use="optional" default="false" />
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element name="Tags" minOccurs="1" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <!--<xs:element name="Tag" type="tagsEnum" minOccurs="1" maxOccurs="unbounded" />-->
            <xs:element name="Tag" type="xs:anyURI" minOccurs="1" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="status" type="statusEnum" />
    <xs:attribute name="releaseVersion" type="releaseVersionsEnum" />
    <xs:attribute name="outputType" type="sampleTypesEnum" use="optional" />
    <xs:attribute name="flag" type="xs:NCName" />
    <xs:attribute name="route" type="xs:string" use="required" />
  </xs:complexType>
