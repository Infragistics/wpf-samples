      <xs:element name="ControlGroup" type="ControlGroup_TYPE" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="ControlGroup_TYPE">
    <xs:sequence>
      <xs:element name="displayNameEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="displayNameJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="Controls" minOccurs="1" maxOccurs="1">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Control" type="Control_TYPE" minOccurs="1" maxOccurs="unbounded">
              <xs:keyref name="availableControlFlags" refer="tns:flags">
                <xs:selector xpath="." />
                <xs:field xpath="@flag" />
              </xs:keyref>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
    <xs:attribute name="name" type="controlsGroupsEnum" use="required" />
  </xs:complexType>
  <xs:complexType name="Control_TYPE">
    <xs:sequence>
      <xs:element name="displayName" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="description" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="icon" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="welcome" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="assembly" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
      <xs:element name="Categories" minOccurs="1" maxOccurs="1">        
        <xs:complexType>
          <xs:sequence>
            <xs:element name="Category" type="Category_TYPE" minOccurs="1" maxOccurs="unbounded" />
          </xs:sequence>
        </xs:complexType>
      </xs:element>      
      <xs:element name="controlPageEN" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />  
      <xs:element name="controlPageJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />     
    </xs:sequence>
    <xs:attribute name="name" type="controlsEnum" use="required" />
    <xs:attribute name="route" type="xs:string" use="required" />
    <xs:attribute name="status" type="statusEnum" />
    <xs:attribute name="releaseVersion" type="releaseVersionsEnum" />
    <xs:attribute name="flag" type="xs:NCName" />
  </xs:complexType>
  <xs:complexType name="Category_TYPE">
