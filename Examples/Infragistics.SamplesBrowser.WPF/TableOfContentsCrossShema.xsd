      <xs:element name="displayNameJA" type="LocalizableText" minOccurs="1" maxOccurs="unbounded" />
      <xs:element name="description" type="LocalizableText" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="icon" type="xs:anyURI" minOccurs="0" maxOccurs="1" />
