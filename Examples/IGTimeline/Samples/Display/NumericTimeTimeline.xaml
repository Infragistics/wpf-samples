    <Grid x:Name="LayoutRoot" Background="{x:Null}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto" />
            <RowDefinition Height="*" />
        </Grid.RowDefinitions>
        <!-- #BEGIN SNIPPET# -->
        <ig:XamTimeline x:Name="Timeline" 
                        Margin="0,0,0,0" VerticalAlignment="Stretch"
                        Grid.Row="1">
            <ig:XamTimeline.Axis>
                <ig:NumericTimeAxis ScrollScale="0.50" ScrollPosition="0.50" Minimum="-850" 
                                    Maximum="1500" AutoRange="False" ShowLabels="False" />
            </ig:XamTimeline.Axis>
            <ig:XamTimeline.PreviewAxis>
                <ig:PreviewAxis ShowLabels="True" />
            </ig:XamTimeline.PreviewAxis>
            <ig:XamTimeline.Series>
                <ig:NumericTimeSeries x:Name="RomanEmpireTimelineSeries" Fill="Red" Position="BottomOrRight" />
                <ig:NumericTimeSeries x:Name="RomanKingdomTimelineSeries"  Fill="#09B030" Position="BottomOrRight" />
                <ig:NumericTimeSeries x:Name="ByzantineTimelineSeries"  Fill="#A00BC9"/>
                <ig:NumericTimeSeries x:Name="MedievalRomeTimelineSeries"  Fill="#7F7F7F" Position="BottomOrRight" />
            </ig:XamTimeline.Series>
        </ig:XamTimeline>
        <!-- #END SNIPPET# -->

        <TextBlock x:Name="SampleTitle" VerticalAlignment="Stretch" Grid.Row="0" Margin="0,0,0,12" Style="{StaticResource SamplesTitleStyle}"/>

    </Grid>
</igFramework:SampleContainer>
