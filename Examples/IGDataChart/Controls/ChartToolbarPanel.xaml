                                <VisualState x:Name="Pressed" />
                                <VisualState x:Name="Disabled" />
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused" />
                                <VisualState x:Name="Unfocused" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle StrokeThickness="2" Fill="#00E0D7CC" />
                        <Path x:Name="normal" Fill="{TemplateBinding Foreground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,4,0,4"  Stretch="Fill" Opacity="1" Data="M 68,30 C68,30 62,36 62,36 62,36 64,38 64,38 64,38 68,34 68,34 68,34 72,38 72,38 72,38 74,36 74,36 74,36 68,30 68,30 z" RenderTransformOrigin="0.5,0.5">
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <RotateTransform Angle="270" />
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
     <Style x:Key="ChartPanRightButton" TargetType="Button" BasedOn="{StaticResource ChartButton}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid Margin="{TemplateBinding Padding}">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualStateGroup.Transitions>
                                    <VisualTransition GeneratedDuration="0:0:0.1" />
                                </VisualStateGroup.Transitions>
                                <VisualState x:Name="Normal" />
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Duration="0" To="#FF353230" Storyboard.TargetProperty="(Path.Fill).(SolidColorBrush.Color)" Storyboard.TargetName="normal" />
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed" />
                                <VisualState x:Name="Disabled" />
                            </VisualStateGroup>
                            <VisualStateGroup x:Name="FocusStates">
                                <VisualState x:Name="Focused" />
                                <VisualState x:Name="Unfocused" />
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                        <Rectangle StrokeThickness="2" Fill="#00E0D7CC" />
                        <Path x:Name="normal" Fill="{TemplateBinding Foreground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="0,4,0,4"  Stretch="Fill" Opacity="1" Data="M 68,30 C68,30 62,36 62,36 62,36 64,38 64,38 64,38 68,34 68,34 68,34 72,38 72,38 72,38 74,36 74,36 74,36 68,30 68,30 z" RenderTransformOrigin="0.5,0.5">
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <RotateTransform Angle="90" />
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style x:Key="ChartToolbarPanelVerticalStyle" TargetType="controls:ChartToolbarPanel">
        <Setter Property="Height" Value="Auto" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:ChartToolbarPanel">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Orientation="Vertical">
                                <ToggleButton x:Name="ZoomMode" Content="ZoomMode" Style="{StaticResource ChartZoomToggleButton}" />
                                <Rectangle Height="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                                <Button x:Name="ZoomIn" Content="ZoomIn" Style="{StaticResource ChartZoomInButton}" />
                                <Button x:Name="ZoomOut" Content="ZoomOut" Style="{StaticResource ChartZoomOutButton}" />
                             
                                <Button x:Name="FitHorizontally" Content="FitHorizontally" Style="{StaticResource ChartFitHorizontallyButton}" />
                                <Button x:Name="FitVertically" Content="FitVertically" Style="{StaticResource ChartFitVerticallyButton}" />
                                <Button x:Name="FitBoth" Content="FitAll" Style="{StaticResource ChartFitBothButton}" />
                               
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <ToggleButton x:Name="PanMode" Content="PanMode" Style="{StaticResource ChartPanToggleButton}" />
                                <Rectangle Height="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                                <Button x:Name="PanUp" Content="PanUp" Style="{StaticResource ChartPanUpButton}" />
                                <Button x:Name="PanDown" Content="PanDown" Style="{StaticResource ChartPanDownButton}" />
                                <Button x:Name="PanLeft" Content="PanLeft" Style="{StaticResource ChartPanLeftButton}" />
                                <Button x:Name="PanRight" Content="PanRight" Style="{StaticResource ChartPanRightButton}" />
                           
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <ToggleButton x:Name="CrosshairVisibility" Style="{StaticResource ChartCrosshairToggleButton}" />
                                <Rectangle Height="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <ToggleButton x:Name="GridVisibility" Style="{StaticResource ChartGridToggleButton}" />
                                <Rectangle Height="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <ToggleButton x:Name="LegendVisibility" Style="{StaticResource ChartLegendToggleButton}" />
                                <Rectangle Height="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                            </StackPanel>
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="ChartToolbarPanelHorizontalStyle" TargetType="controls:ChartToolbarPanel">
        <Setter Property="Height" Value="Auto" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="controls:ChartToolbarPanel">
                    <Border Background="{TemplateBinding Background}" BorderBrush="{TemplateBinding BorderBrush}" BorderThickness="{TemplateBinding BorderThickness}">
                        <StackPanel Orientation="Horizontal">
                            <StackPanel Orientation="Vertical" VerticalAlignment="Center">
                                <ToggleButton x:Name="ZoomMode" Content="ZoomMode" Style="{StaticResource ChartZoomToggleButton}" />
                                <ToggleButton x:Name="PanMode" Content="PanMode" Style="{StaticResource ChartPanToggleButton}" />
                            </StackPanel>
                            <Rectangle Width="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />

                            <StackPanel Orientation="Vertical" VerticalAlignment="Center">
                                <Button x:Name="ZoomIn" Content="ZoomIn" Style="{StaticResource ChartZoomInButton}" />
                                <Button x:Name="ZoomOut" Content="ZoomOut" Style="{StaticResource ChartZoomOutButton}" />
                            </StackPanel>
                            <Rectangle Width="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
 
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <Button x:Name="FitHorizontally" Content="FitHorizontally" Style="{StaticResource ChartFitHorizontallyButton}" />
                                <Button x:Name="FitVertically" Content="FitVertically" Style="{StaticResource ChartFitVerticallyButton}" />
                                <Button x:Name="FitBoth" Content="FitAll" Style="{StaticResource ChartFitBothButton}" />
                            </StackPanel>
                            <Rectangle Width="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />
                          
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <Button x:Name="PanLeft" Content="PanLeft" Style="{StaticResource ChartPanLeftButton}" />
                                <Button x:Name="PanRight" Content="PanRight" Style="{StaticResource ChartPanRightButton}" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical" VerticalAlignment="Center">
                                <Button x:Name="PanUp" Content="PanUp" Style="{StaticResource ChartPanUpButton}" />
                                <Button x:Name="PanDown" Content="PanDown" Style="{StaticResource ChartPanDownButton}"  />
                            </StackPanel>
                            <Rectangle Width="1" Fill="Black" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Margin="2" />

                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                <ToggleButton x:Name="CrosshairVisibility" Style="{StaticResource ChartCrosshairToggleButton}" />
                                <ToggleButton x:Name="GridVisibility" Style="{StaticResource ChartGridToggleButton}" />
                                <ToggleButton x:Name="LegendVisibility" Style="{StaticResource ChartLegendToggleButton}" />
                            </StackPanel>
 
                        </StackPanel>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style BasedOn="{StaticResource ChartToolbarPanelVerticalStyle}" TargetType="controls:ChartToolbarPanel" />

    <LinearGradientBrush x:Key="ChartToolbarPanelTooltipFill" EndPoint="0.5,1" StartPoint="0.5,0" Opacity="0.90">
        <GradientStop Color="#FF181615" Offset="1" />
        <GradientStop Color="#FF2A2825" />
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ChartToolbarPanelTooltipStroke" Color="#FF181615" Opacity="0.95" />
    <Style x:Key="ChartToolbarPanelToolTipStyle" TargetType="ContentControl">
        <Setter Property="Foreground" Value="{StaticResource ChartToolbarPanelForeground}" />
        <Setter Property="Padding" Value="7,4,7,6" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="MaxWidth" Value="120" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Border BorderBrush="{StaticResource ChartToolbarPanelTooltipStroke}" Background="{StaticResource ChartToolbarPanelTooltipFill}" CornerRadius="3" BorderThickness="1">
                        <Border.Effect>
                            <DropShadowEffect BlurRadius="8" Opacity="0.4" ShadowDepth="3" />
                        </Border.Effect>
                        <ContentPresenter Margin="{TemplateBinding Padding}" Content="{TemplateBinding Content}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
 
    <!-- ====================================================================================== -->

</ResourceDictionary>
